{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"segments": [
				{
					"foreground": "#14a10c",
					"style": "plain",
					"template": "\u279c",
					"type": "text"
				},
				{
					"foreground": "#95b6ff",
					"properties": {
						"style": "folder"
					},
					"style": "plain",
					"template": "  {{ .Path }} ",
					"type": "path"
				},
				{
					"foreground": "#ff4f53",
					"properties": {
						"branch_icon": ""
					},
					"style": "plain",
					"template": "<#4e84f8>git:(</>{{ .HEAD }}<#4e84f8>)</>",
					"type": "git"
				},
				{
					"foreground": "#ff0004",
					"style": "plain",
					"template": " \u2717",
					"type": "exit"
				}
			],
			"type": "prompt"
		}
	],
	"final_space": true,
	"version": 2
}
